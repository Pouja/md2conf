{
  "name": "md2conf",
  "description": "Markdown to Confluence markup.",
  "version": "1.3.5",
  "author": {
    "name": "Zac Anger",
    "email": "zac@zacanger.com",
    "url": "http://zacanger.com"
  },
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js",
    "cli.js"
  ],
  "bin": "./cli.js",
  "scripts": {
    "preversion": "npm t",
    "test": "run-s -s test:lint test:cover",
    "test:lint": "eslint -c .eslintrc.json *.js",
    "test:tape": "tape test.js",
    "test:cover": "nyc npm run test:tape"
  },
  "homepage": "https://github.com/zacanger/md2conf",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/md2conf.git"
  },
  "bugs": {
    "url": "https://github.com/zacanger/md2conf/issues"
  },
  "keywords": [
    "confluence",
    "convert",
    "markdown",
    "markup",
    "md"
  ],
  "dependencies": {
    "get-stdin": "5.0.1",
    "marked": "0.3.2",
    "min-qs": "1.3.0",
    "min-util": "2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.6.1",
    "eslint-config-zacanger": "3.4.1",
    "eslint-plugin-babel": "5.2.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-unicorn": "6.0.1",
    "npm-run-all": "4.1.3",
    "nyc": "13.0.1",
    "tape": "4.9.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "nyc": {
    "include": [
      "index.js"
    ],
    "sourceMap": true,
    "instrument": true,
    "reporter": [
      "text",
      "html",
      "json"
    ]
  }
}
